# Configuration pour déploiement AWS

# Nom du bucket S3 (doit être unique globalement)
S3_BUCKET_NAME=travelmate-frontend

# ID de distribution CloudFront (optionnel)
CLOUDFRONT_DISTRIBUTION_ID=

# URL du backend API (à configurer après déploiement backend)
VITE_API_URL=https://votre-backend.aws.com

# Variables Firebase (à configurer)
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=
VITE_FIREBASE_MEASUREMENT_ID=

# Google Maps API Key
VITE_GOOGLE_MAPS_API_KEY=


